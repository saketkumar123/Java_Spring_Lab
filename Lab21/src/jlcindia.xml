<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
  
<bean id="ao1" class="com.jlcindia.spring.A">
	<property name="a" value="10"/>
	<property name="msg" value="AAA"/>
</bean>

<bean id="aobj" class="com.jlcindia.spring.A">
	<property name="a" value="11"/>
	<property name="msg" value="AAA-A"/>
</bean>

<bean id="bo" class="com.jlcindia.spring.B">
	<constructor-arg value="20"/>
	<constructor-arg value = "BBB"/>
</bean>

<bean id="bobj" class="com.jlcindia.spring.B">
	<constructor-arg value="21"/>
	<constructor-arg value = "BBB-B"/>
</bean>

<bean id="hello" class="com.jlcindia.spring.Hello" autowire = "constructor"/>
	
</beans>